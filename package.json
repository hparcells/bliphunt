{
  "name": "bliphunt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "cross-env NODE_ENV=development PORT=8000 ts-node --project tsconfig.server.json ./src/server/server.ts",
    "lint": "next lint",
    "jest": "jest --maxWorkers=50% --coverage",
    "cypress": "cross-env NODE_ENV=development cypress open",
    "cypress:headless": "cross-env NODE_ENV=development cypress run",
    "test": "npm run jest && npm run cypress:headless",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build": "npm test && turbo build:next && turbo build:server",
    "start": "cross-env NODE_ENV=production PORT=8005 node dist/server/server.js",
    "production": "npm run build && npm start"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@mantine/core": "^5.10.5",
    "@mantine/form": "^5.10.5",
    "@mantine/hooks": "^5.10.5",
    "@mantine/next": "^5.10.5",
    "@sentry/nextjs": "^7.105.0",
    "@tabler/icons": "^2.6.0",
    "axios": "^1.3.3",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "clsx": "^1.2.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "log-type": "^1.0.0",
    "mongodb-memory-server": "^8.11.5",
    "mongoose": "^6.9.2",
    "next": "13.2.3",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-ga4": "^2.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.1.6",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "@types/react": "18.0.28",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/parser": "^5.54.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.7.0",
    "eslint": "^8.34.0",
    "eslint-config-next": "13.2.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "turbo": "^1.8.3",
    "typescript": "^4.9.5"
  }
}
